{
	"name": "pl_load_dimdivision",
	"properties": {
		"activities": [
			{
				"name": "Copy data for dimivision",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT DISTINCT tvs.DivisionName,tvs.DivisionID,geo.PKGeographyID as FKGeographyID\nFROM [stg].[StagingTVS] as tvs\nINNER JOIN \nDW.DimGeography as geo on tvs.DivisionPostalCode=geo.PostalCode\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"DivisionID"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_tvsdw_naveen",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sink_dimdivision",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Naveen/upsert"
		},
		"annotations": []
	}
}